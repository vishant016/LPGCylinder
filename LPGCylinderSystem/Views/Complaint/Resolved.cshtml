@model List<LPGCylinderSystem.Models.Complaint>

@{
    ViewData["Title"] = "Resolved";
}


@foreach (var item in Model)
{
<h1>Resolved</h1>

<div>
    <h4>Complaint</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
           Complaint_Id
        </dt>
        <dd class = "col-sm-10">
            @item.Complaint_Id
        </dd>
        <dt class = "col-sm-2">
            Category
        </dt>
        <dd class = "col-sm-10">
            @item.Category
        </dd>
        <dt class = "col-sm-2">
            Description
        </dt>
        <dd class = "col-sm-10">
            @item.Description
        </dd>
       
        <dt class = "col-sm-2">
            Remark
        </dt>
        <dd class = "col-sm-10">
            @item.Remark
        </dd>
        <dt class = "col-sm-2">
            Booking_Id
        </dt>
        <dd class = "col-sm-10">
            @item.Booking_Id
        </dd>
    </dl>
</div>

}